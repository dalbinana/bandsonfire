
  <h2>List of bands from <%= current_user.name %> <%= link_to "Add bands", new_user_band_path, :class => "btn btn-success" %></h2>
    <table class="table table-hover">
    <tr><th>Picture</th><th>Name</th><th>Spotify ID</th></tr>
    <% @bands.each do |band| %>
    
     <tr><td><img src=<%= band.picture %>> </td><td><%= band.name %> </td> <td><%= band.spotifyID %> </td></tr>
    
    <% end %>
    </table>
 

